{
  "family": "magento-webserver_{{TASK_DEFINITION_TAG}}",
  "executionRoleArn": "arn:aws:iam::520431583319:role/MagentoECSTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::520431583319:role/MagentoECSTaskRole",
  "containerDefinitions": [
    {
      "name": "php",
      "image": "{{AWS_REGISTRY_URL}}/kfd-magento/php:{{DOCKER_IMAGE_TAG}}",
      "cpu": 0,
      "memoryReservation": {{ECS_PHP_MEMORY_RESERVATION}},
      "memory": {{ECS_PHP_MEMORY}},
      "portMappings": [],
      "essential": true,
      "mountPoints": [
        {
          "sourceVolume": "efs-media",
          "containerPath": "/var/www/html/pub/media"
        },
        {
          "sourceVolume": "efs-export",
          "containerPath": "/var/www/html/var/export"
        },
        {
          "sourceVolume": "efs-flags",
          "containerPath": "/var/www/html/var/flags"
        },
        {
          "sourceVolume": "efs-amasty-feed",
          "containerPath": "/var/www/html/var/amasty/feed"
        },
        {
          "sourceVolume": "pub-static",
          "containerPath": "/var/www/html/pub/static"
        },
        {
          "sourceVolume": "php-sock",
          "containerPath": "/var/run/php"
        }
      ],
      "volumesFrom": [],
      "environment": [
        {
          "name": "MAGE_DEPLOY__ENV_TEMPLATE",
          "value": "prod"
        },
        {
          "name": "MAGE_DEPLOY__UPGRADE",
          "value": "{{MAGE_DEPLOY__UPGRADE}}"
        },
        {
          "name": "MAGE_DEPLOY__ALLOW_OUTDATED_DB",
          "value": "{{MAGE_DEPLOY__ALLOW_OUTDATED_DB}}"
        },
        {
          "name": "MAGE__CACHE_ID_PREFIX",
          "value": "69d_"
        },
        {
          "name": "MAGE__CACHE_PRELOAD_DISABLE",
          "value": "1"
        },
        {
          "name": "PHP_FPM_PM_START_SERVERS",
          "value": "{{PHP_FPM_PM_START_SERVERS}}"
        },
        {
          "name": "PHP_FPM_PM_MIN_SPARE_SERVERS",
          "value": "{{PHP_FPM_PM_MIN_SPARE_SERVERS}}"
        },
        {
          "name": "PHP_FPM_PM_MAX_SPARE_SERVERS",
          "value": "{{PHP_FPM_PM_MAX_SPARE_SERVERS}}"
        },
        {
          "name": "PHP_FPM_PM_MAX_CHILDREN",
          "value": "{{PHP_FPM_PM_MAX_CHILDREN}}"
        },
        {
          "name": "PHP_MEMORY_LIMIT",
          "value": "{{PHP_MEMORY_LIMIT}}"
        },
        {
          "name": "MAGE__ENABLE_DEBUG_LOGGING",
          "value": "false"
        },
        {
          "name": "MAGE__ENABLE_SYSLOG_LOGGING",
          "value": "false"
        },
        {
          "name": "CONFIG__DEFAULT__SMILE_ELASTICSUITE_CORE_BASE_SETTINGS__INDICES__SETTINGS_ALIAS",
          "value": "elasticsuite_{{ENVIRONMENT}}"
        }
      ],
      "secrets": [
        {
          "name": "MAGE__BACKEND_FRONTNAME",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/magento/backend_frontname"
        },
        {
          "name": "MAGE__DB_HOST",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/database/host"
        },
        {
          "name": "MAGE__DB_NAME",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/database/name"
        },
        {
          "name": "MAGE__DB_USER",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/database/username"
        },
        {
          "name": "MAGE__DB_PASSWORD",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/database/password"
        },
        {
          "name": "MAGE__KEY",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/magento/key"
        },
        {
          "name": "MAGE__CACHE_REDIS_SERVER",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/redis/host"
        },
        {
          "name": "MAGE__CACHE_REDIS_REPLICA_HOST",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/redis/replica_host"
        },
        {
          "name": "MAGE__PAGE_CACHE_REDIS_SERVER",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/redis/host"
        },
        {
          "name": "MAGE__PAGE_CACHE_REDIS_REPLICA_HOST",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/redis/replica_host"
        },
        {
          "name": "MAGE__SESSION_REDIS_HOST",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/session-redis/host"
        },
        {
          "name": "CONFIG__DEFAULT__CATALOG__SEARCH__ELASTICSEARCH7_SERVER_HOSTNAME",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/elasticsearch7/host"
        },
        {
          "name": "CONFIG__DEFAULT__CATALOG__SEARCH__ELASTICSEARCH7_SERVER_PORT",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/elasticsearch7/port"
        },
        {
          "name": "CONFIG__DEFAULT__CATALOG__SEARCH__ELASTICSEARCH7_ENABLE_AUTH",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/elasticsearch7/auth"
        },
        {
          "name": "CONFIG__DEFAULT__CATALOG__SEARCH__ELASTICSEARCH7_USERNAME",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/elasticsearch7/username"
        },
        {
          "name": "CONFIG__DEFAULT__CATALOG__SEARCH__ELASTICSEARCH7_PASSWORD",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/elasticsearch7/password"
        },
        {
          "name": "CONFIG__DEFAULT__SMILE_ELASTICSUITE_CORE_BASE_SETTINGS__ES_CLIENT__SERVERS",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/elasticsearch7/servers"
        },
        {
          "name": "CONFIG__DEFAULT__SMILE_ELASTICSUITE_CORE_BASE_SETTINGS__ES_CLIENT__HTTP_AUTH_USER",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/elasticsearch7/username"
        },
        {
          "name": "CONFIG__DEFAULT__SMILE_ELASTICSUITE_CORE_BASE_SETTINGS__ES_CLIENT__HTTP_AUTH_PWD",
          "valueFrom": "arn:aws:ssm:eu-west-2:520431583319:parameter/{{ENVIRONMENT}}/magento/elasticsearch7/password"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/magento-webserver-{{ENVIRONMENT}}-php",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "ecs",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        }
      }
    },
    {
      "name": "nginx",
      "image": "{{AWS_REGISTRY_URL}}/kfd-magento/nginx:{{DOCKER_IMAGE_TAG}}",
      "cpu": 0,
      "memoryReservation": {{ECS_NGINX_MEMORY_RESERVATION}},
      "memory": {{ECS_NGINX_MEMORY}},
      "links": [
        "php"
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "protocol": "tcp"
        },
        {
          "containerPort": 8137,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "mountPoints": [
        {
          "sourceVolume": "efs-media",
          "containerPath": "/var/www/html/pub/media",
          "readOnly": true
        },
        {
          "sourceVolume": "pub-static",
          "containerPath": "/var/www/html/pub/static",
          "readOnly": true
        },
        {
          "sourceVolume": "php-sock",
          "containerPath": "/var/run/php",
          "readOnly": true
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/magento-webserver-{{ENVIRONMENT}}-nginx",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "ecs",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        }
      }
    }
  ],
  "volumes": [
    {
      "name": "efs-media",
      "efsVolumeConfiguration": {
        "fileSystemId": "{{MEDIA_FILESYSTEM_ID}}",
        "rootDirectory": "/media",
        "transitEncryption": "ENABLED"
      }
    },
    {
      "name": "efs-export",
      "efsVolumeConfiguration": {
        "fileSystemId": "{{MEDIA_FILESYSTEM_ID}}",
        "rootDirectory": "/export",
        "transitEncryption": "ENABLED"
      }
    },
    {
      "name": "efs-flags",
      "efsVolumeConfiguration": {
        "fileSystemId": "{{MEDIA_FILESYSTEM_ID}}",
        "rootDirectory": "/flags/{{ENVIRONMENT}}",
        "transitEncryption": "ENABLED"
      }
    },
    {
      "name": "efs-amasty-feed",
      "efsVolumeConfiguration": {
        "fileSystemId": "{{MEDIA_FILESYSTEM_ID}}",
        "rootDirectory": "/amasty-feed",
        "transitEncryption": "ENABLED"
      }
    },
    {
      "name": "pub-static",
      "dockerVolumeConfiguration": {
        "scope": "task",
        "driver": "local"
      }
    },
    {
      "name": "php-sock"
    }
  ],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2"
  ]
}
